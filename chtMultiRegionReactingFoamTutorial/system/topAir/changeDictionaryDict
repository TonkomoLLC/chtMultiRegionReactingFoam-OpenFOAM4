U
{
    internalField   uniform (0 0 0);
    boundaryField
    {
        ".*"
        {
            type            fixedValue;
            value           uniform (0 0 0);
        }
        minX
        {
            type            fixedValue;
            value           uniform (0.5 0 0);
        }
        maxX
        {
            type            fixedValue;
            value           uniform (-0.5 0 0);
        }
    }
}

T
{
    internalField   uniform 300;
    boundaryField
    {
        ".*"
        {
            type            zeroGradient;
        }
        minX
        {
            type            fixedValue;
            value           uniform 300;
        }
        maxX
        {
            type            fixedValue;
            value           uniform 300;
        }
        maxY
        {
            type            inletOutlet;
            inletValue      uniform 300;
            value           uniform 300;
        }        
        "topAir_to_.*"
        {
            type            compressible::turbulentTemperatureRadCoupledMixed;
            Tnbr            T;
            kappaMethod     fluidThermo;
            QrNbr           none;
            Qr              Qr;
            value           uniform 300;
        }
    }
}

